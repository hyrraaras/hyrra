package ödevler;

public class WhileDöngüsüÖdev {

	public static void main(String[] args) {
		//while dögüsü ile piramit oluşturma
		int a = 10; 
		while (a >= 1) { 
		    int b = 1; 
		    while (b <= a) { 
		        System.out.print("*"); 
		        b++; 
		    } 
		    System.out.println(); 
		    a--; 
		}

	}

}
