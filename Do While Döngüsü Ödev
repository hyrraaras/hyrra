package ödevler;

public class DoWhileDöngüsüÖdev {

	public static void main(String[] args) {
		//do while dögüsü ile piramit oluşturma
		int a = 10; 
		do {
		    int b = 1; 
		    do {
		        System.out.print("*"); 
		        b++; 
		    } while (b <= a); 
		    System.out.println(); 
		    a--; 
		} while (a >= 1);

	}

}
